{
  "$schema": "http://json.schemastore.org/prettierrc",
  "bracketSpacing": true,
  "printWidth": 140,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "all",
  "singleQuote": false,
  "semi": true,
  "quoteProps": "as-needed",
  "arrowParens": "avoid",
  "rangeStart": 0,
  "endOfLine": "lf"
}
